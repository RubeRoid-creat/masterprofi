{
  "name": "masterprofi",
  "version": "1.0.0",
  "description": "MasterProfi Service Platform",
  "scripts": {
    "install:all": "npm install && cd backend && npm install && cd ../web-admin && npm install",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run start:dev",
    "dev:frontend": "cd web-admin && npm run dev",
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "cd backend && npm run build",
    "build:frontend": "cd web-admin && npm run build",
    "test": "npm run test:backend && npm run test:frontend",
    "test:backend": "cd backend && npm run test",
    "test:frontend": "cd web-admin && npm run test",
    "lint": "npm run lint:backend && npm run lint:frontend",
    "lint:backend": "cd backend && npm run lint",
    "lint:frontend": "cd web-admin && npm run lint"
  },
  "keywords": ["saas", "mlm", "service-platform", "nestjs", "react"],
  "author": "MasterProfi Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}

