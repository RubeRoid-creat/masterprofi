{
  "name": "masterprofi-mobile",
  "version": "1.0.0",
  "main": "expo/AppEntry.js",
  "scripts": {
    "start": "npx expo start",
    "start:clean": "npx expo start --clear",
    "android": "npx expo run:android",
    "ios": "npx expo run:ios",
    "web": "npx expo start --web",
    "analyze": "node scripts/analyze-bundle.js",
    "bundle:android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --sourcemap-output android/app/src/main/assets/index.android.bundle.map",
    "bundle:ios": "react-native bundle --platform ios --dev false --entry-file index.js --bundle-output ios/main.jsbundle --sourcemap-output ios/main.jsbundle.map",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "e2e:build:ios": "detox build --configuration ios.sim.debug",
    "e2e:build:android": "detox build --configuration android.emu.debug",
    "e2e:test:ios": "detox test --configuration ios.sim.debug",
    "e2e:test:android": "detox test --configuration android.emu.debug",
    "e2e:test:ios:release": "detox test --configuration ios.sim.release",
    "e2e:test:android:release": "detox test --configuration android.emu.release",
    "generate:icons": "node scripts/generate-icons.js",
    "generate:splash": "node scripts/generate-splash.js",
    "generate:screenshots": "node scripts/generate-screenshots.js",
    "generate:release-notes": "node scripts/generate-release-notes.js",
    "configure:ota": "node scripts/configure-ota.js",
    "build:ios": "eas build --platform ios",
    "build:android": "eas build --platform android",
    "submit:ios": "eas submit --platform ios",
    "submit:android": "eas submit --platform android",
    "update:dev": "eas update --branch development",
    "update:preview": "eas update --branch preview",
    "update:production": "eas update --branch production"
  },
  "dependencies": {
    "@expo/metro-runtime": "~3.1.3",
    "@nozbe/watermelondb": "^0.28.0",
    "@nozbe/with-observables": "^1.6.0",
    "@react-native-async-storage/async-storage": "1.21.0",
    "@react-native-community/netinfo": "^11.1.0",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@react-navigation/stack": "^6.3.20",
    "@reduxjs/toolkit": "^2.0.1",
    "@types/uuid": "^9.0.7",
    "expo": "~50.0.0",
    "expo-asset": "~9.0.2",
    "expo-av": "~13.10.6",
    "expo-build-properties": "~0.11.1",
    "expo-camera": "~14.1.3",
    "expo-crypto": "~12.8.1",
    "expo-device": "~5.9.4",
    "expo-document-picker": "~11.10.1",
    "expo-file-system": "~16.0.9",
    "expo-image": "~1.10.6",
    "expo-image-manipulator": "~11.8.0",
    "expo-image-picker": "~14.7.1",
    "expo-linear-gradient": "^15.0.7",
    "expo-local-authentication": "~13.8.0",
    "expo-location": "~16.5.5",
    "expo-notifications": "~0.27.8",
    "expo-sqlite": "~13.4.0",
    "expo-task-manager": "~11.7.3",
    "expo-updates": "~0.24.13",
    "nativewind": "^2.0.11",
    "react": "^18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.48.2",
    "react-native": "^0.73.6",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-get-random-values": "~1.8.0",
    "react-native-maps": "1.10.0",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-svg": "13.4.0",
    "react-native-web": "~0.19.6",
    "react-redux": "^9.0.4",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@babel/plugin-proposal-decorators": "^7.28.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.4.2",
    "@types/jest": "^29.5.11",
    "@types/react": "~18.2.45",
    "detox": "^20.14.0",
    "jest": "^29.7.0",
    "jest-react-native": "^18.0.0",
    "react-test-renderer": "18.2.0",
    "tailwindcss": "3.3.2",
    "typescript": "^5.1.3"
  },
  "private": true
}
